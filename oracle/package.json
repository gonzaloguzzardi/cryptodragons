    "ganache-cli": "ganache-cli -p 8545 -v -m \"trade nothing tag piece anger smoke clay reveal burst behave dose brave\"",
    "stats": "webpack --profile --json > stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "body-parser": "latest",
    "express": "latest",
    "fs": "latest",
    "node-cron": "latest",
    "request": "latest",
    "mongodb": "latest",
    "web3": "1.0.0-beta.37",
    "loom-js": "latest",
    "bn": "latest"
  }
